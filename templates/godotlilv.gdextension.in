[godot-lilv]

version = "${version}"
build = "${build}"

[configuration]

entry_symbol = "godot_lilv_library_init"
compatibility_minimum = 4.3
reloadable = true

[libraries]

macos.debug = "res://addons/lilv/bin/macos/macos.framework/liblilvgodot.macos.template_debug"
macos.release = "res://addons/lilv/bin/macos/macos.framework/liblilvgodot.macos.template_release"
windows.debug.x86_64 = "res://addons/lilv/bin/windows/liblilvgodot.windows.template_debug.dev.x86_64.dll"
windows.release.x86_64 = "res://addons/lilv/bin/windows/liblilvgodot.windows.template_release.x86_64.dll"
linux.debug.x86_64 = "res://addons/lilv/bin/linux/liblilvgodot.linux.template_debug.dev.x86_64.so"
linux.release.x86_64 = "res://addons/lilv/bin/linux/liblilvgodot.linux.template_release.x86_64.so"
